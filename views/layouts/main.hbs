{{> head }}

<body>
  <header class="header transition">
    <div class="wrap">
      <div class="logo-mini"></div>
      <ul class="header-info transition-06">
        <li class="header-instagram">
          <a class="transition" href="https://www.instagram.com/oorun_art" target="_blank">
            <span class="transition">
              <svg class="instagram" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 50 50" height="30px" width="30px">
                <path
                  d="M16 3C8.832 3 3 8.832 3 16v18c0 7.168 5.832 13 13 13h18c7.168 0 13-5.832 13-13V16c0-7.168-5.832-13-13-13H16zm0 2h18c6.086 0 11 4.914 11 11v18c0 6.086-4.914 11-11 11H16C9.914 45 5 40.086 5 34V16C5 9.914 9.914 5 16 5zm21 6a2 2 0 0 0-2 2 2 2 0 0 0 2 2 2 2 0 0 0 2-2 2 2 0 0 0-2-2zm-12 3c-6.063 0-11 4.937-11 11s4.937 11 11 11 11-4.937 11-11-4.937-11-11-11zm0 2c4.982 0 9 4.018 9 9s-4.018 9-9 9-9-4.018-9-9 4.018-9 9-9z"
                  font-weight="400" font-family="sans-serif" white-space="normal" overflow="visible" />
              </svg>
            </span>
          </a>
        </li>
        <li class="header-info-tel">
          <a class="transition" href="tel:+380637011056">
            <span class="transition">+38 (063) 701-10-56</span>
          </a>
        </li>
      </ul>
      <div class="toogle transition">
        <span class="menu-line transition"></span>
        <span class="menu-line transition"></span>
        <span class="menu-line transition"></span>
      </div>
      <a class="card" href="/card">
        <svg class="card-icon transition" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"
          xmlns:xlink="http://www.w3.org/1999/xlink" focusable="false">
          <path
            d="M12 .1c2.9 0 4.9 1.4 4.9 3.8v.6h3.8v5.1c0 4.7 2.1 13.3.5 13.3-.7 0-5 1.1-9.2 1.1-4.6 0-9.2-1.1-9.2-1.1-1.5 0 .5-8.7.5-13.3V4.4h3.8v-.6c0-2.4 2-3.7 4.9-3.7zm7.6 5.4h-2.7v2.2h-1.1V5.5H8.2v2.2H7.1V5.5H4.4v4c0 1.3-.1 2.9-.4 5.3l-.1.9c-.4 3-.5 3.9-.5 5v1.2c.8.1 1.6.3 2.4.4 2.1.3 4.2.5 6.1.5h.5c1.6 0 3.3-.2 5.2-.5.2 0 2-.4 2.9-.5v-1.4c0-1-.2-1.9-.5-4.7 0-.3-.1-.6-.1-.9-.3-2.4-.4-4-.4-5.3v-4zM12 1.2c-2.3 0-3.8 1-3.8 2.6v.6h7.6v-.6c0-1.7-1.5-2.6-3.8-2.6z">
          </path>
        </svg>
        <span class="count transition">({{ countTotal }})</span>
      </a>
    </div>
    {{> navbar }}
  </header>

  <main>
    {{{ body }}}
    {{> contact-form }}
  </main>

  {{> footer }}
</body>

</html>